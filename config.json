{
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended",
    "plugin:import/recommended",
    "plugin:import/typescript"
  ],
  "plugins": ["@typescript-eslint", "prettier", "import"],
  "rules": {
    "no-unused-vars": 0,
    "prettier/prettier": "error"
  },
  "parser": "@typescript-eslint/parser"
}
